<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/paper"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#D3F0D2"
    tools:context=".MainActivity">

    <Button
        android:id="@+id/button"
        android:layout_width="112dp"
        android:layout_height="50dp"
        android:backgroundTint="#FFFFFF"
        android:onClick="mainTOselection"
        android:text="@string/join_us"
        android:textColor="#3F9D2F"
        android:textColorLink="#ff6750a4"
        android:textSize="20sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.778" />

    <EditText
        android:id="@+id/setname"
        android:layout_width="283dp"
        android:layout_height="71dp"
        android:autofillHints="Enter Name"
        android:ellipsize="start"
        android:ems="10"
        android:hint="Enter Name"
        android:inputType="text"
        android:textColor="#4CAF50"
        android:textColorHint="#5D5D5D"
        android:textSize="20sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.652"
        tools:ignore="HardcodedText,LabelFor" />

    <ImageView
        android:id="@+id/ImageView2"
        android:layout_width="406dp"
        android:layout_height="287dp"
        android:contentDescription="@string/todo"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintDimensionRatio="@string/join_us"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.4"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.244"
        app:srcCompat="@drawable/cc1"
        tools:srcCompat="@drawable/cc1" />

</androidx.constraintlayout.widget.ConstraintLayout>